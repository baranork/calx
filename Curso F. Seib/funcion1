function sumar(num1, num2){
    return num1 + num2;
}

function restar(num1, num2){
    return num1 - num2;
}

function dividir(num1, num2){
    if(num2 != 0){
        return num1 / num2
    }
    else{
        return "No se puede dividir por 0";
    }
}

function multiplicar(num1, num2){
    return num1 * num2;
}

function raizCuadrada(num){
    return num / num;
}

function raizCubica(num){
    return num / num / num;
}